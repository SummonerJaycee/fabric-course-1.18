{
  "schemaVersion": 1,
  "id": "mccourse",
  "version": "${version}",

  "name": "MCCourse Fabric Mod",
  "description": "A mod to learn Minecraft Fabric modding",
  "authors": [
    "Astranautical Labs"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/astranautical/fabric-course-1.18"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.astranautical.mccourse.MCCourseMod"
    ]
  },
  "mixins": [
    "mccourse.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
